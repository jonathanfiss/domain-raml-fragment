#%RAML 1.0 DataType 
type: object
properties:
  first_name:
    type: string
    example: John
    required: false
  last_name:
    type: string
    example: Doe
    required: false
  email:
    type: string
    example: johndoe@example.com
    required: false
  status:
    type: string
    example: active
    required: false
